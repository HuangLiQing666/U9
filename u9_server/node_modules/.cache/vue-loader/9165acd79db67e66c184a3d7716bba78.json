nsole.log(111)\r\n            this.$messagebox.prompt(\"请输入年龄\").then((value)=>{\r\n                console.log(value);\r\n            }).catch(err=>{\r\n                console.log(err)\r\n            });\r\n        },\r\n        openToast(){\r\n            // 此函数完成显示mint-ui短消息提示框\r\n            this.$toast({\r\n                message:\"操作成功\",\r\n                duration:500,\r\n                position:\"bottom\",\r\n                iconClass:\"iconfont icon-zhijiayou\"\r\n            });\r\n        },\r\n        openAlert(){\r\n            // //交互的提示框\r\n            // this.$messagebox({\r\n            //     title:\"消息\",\r\n            //     message:\"执行操作成功\"\r\n            // })\r\n            this.$messagebox(\"\",\"执行成功\");\r\n        },\r\n        openConfirm(){\r\n            // 显示确认的消息框\r\n            this.$messagebox.confirm(\"确认删除指定的数据?\")\r\n            .then(result=>{\r\n                // 用户选择确认回调函数\r\n                console.log(result)\r\n            })\r\n            .catch(err=>{\r\n                // 用户选择取消回调函数\r\n                console.log(\"取消\")\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style>\r\n    /* 当前组件专有的样式内容 */\r\n</style>\r\n\r\n"]}]}